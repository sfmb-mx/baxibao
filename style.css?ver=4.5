/* Effeito 01 */
.ico-efx-a .hi-icon {
  box-shadow: 0 0 0 4px #f99d1c;
  -webkit-transition: color 0.3s;
  -moz-transition: color 0.3s;
  transition: color 0.3s; }

.ico-efx-a .hi-icon:after {
  top: -2px;
  left: -2px;
  padding: 2px;
  z-index: -1;
  background: #f99d1c;
  -webkit-transition: -webkit-transform 0.2s, opacity 0.3s;
  -moz-transition: -moz-transform 0.2s, opacity 0.3s;
  transition: transform 0.2s, opacity 0.3s; }

.ico-efx-a.one .hi-icon {
  color: #f3f2f2; }

.ico-efx-a.one .hi-icon:hover {
  color: f99d1c; }

.ico-efx-a.one .hi-icon:hover:after {
  -webkit-transform: scale(1.3);
  -moz-transform: scale(1.3);
  -ms-transform: scale(1.3);
  transform: scale(1.3);
  opacity: 0; }

/* Efeito 2 */
.ico-efx-b .hi-icon {
  color: #fff;
  box-shadow: 0 0 0 3px #f99d1c;
  -webkit-transition: color 0.3s;
  -moz-transition: color 0.3s;
  transition: color 0.3s; }

.ico-efx-b .hi-icon:after {
  top: -2px;
  left: -2px;
  padding: 2px;
  z-index: -1;
  background: #f99d1c;
  -webkit-transition: -webkit-transform 0.2s, opacity 0.2s;
  -moz-transition: -moz-transform 0.2s, opacity 0.2s;
  transition: transform 0.2s, opacity 0.2s; }

/* Effect 2a */
.ico-efx-b.one .hi-icon:hover {
  color: #f99d1c; }

.ico-efx-b.one .hi-icon:hover:after {
  -webkit-transform: scale(0.85);
  -moz-transform: scale(0.85);
  -ms-transform: scale(0.85);
  transform: scale(0.85); }

/* Effect 2b */
.ico-efx-b.two .hi-icon:hover:after {
  -webkit-transform: scale(0);
  -moz-transform: scale(0);
  -ms-transform: scale(0);
  transform: scale(0);
  opacity: 0;
  -webkit-transition: -webkit-transform 0.4s, opacity 0.2s;
